{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "gdb",
      "request": "attach",
      "executable": "${workspaceRoot}/build/kameleon-core.elf",
      "target": ":4242",
      "remote": true,
      "cwd": "${workspaceRoot}",
      "gdbpath": "arm-none-eabi-gdb",
      "autorun": [
        "load ./build/kameleon-core.elf"
      ]
    }    
  ]
}